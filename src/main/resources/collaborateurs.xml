	</bean>

	<bean id="collaborateur3" class="dev.entity.Collaborateur">
		<property name="matricule" value="E070809"></property>
		<property name="role" value="Employe"></property>
	</bean>

	<bean id="collaborateur4" class="dev.entity.Collaborateur">
		<property name="matricule" value="E101112"></property>
		<property name="role" value="Employe"></property>
	</bean>

</beans>
